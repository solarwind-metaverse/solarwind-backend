{
  "name": "solarwind-backend",
  "version": "0.0.1",
  "description": "Solarwind backend",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "ts-node": "ts-node",
    "execute": "node dist/index.js",
    "serve": "tsc && node dist/index.js",
    "build": "tsc",
    "watch": "tsc-watch --silent --onSuccess \"node dist/index.js\"",
    "start": "source .env && source ../solarwind-blockchain/.env && tsc && tsc-watch --silent --onSuccess \"npm run serve\"",
    "start-prod": "tsc && tsc-watch --silent --onSuccess \"npm run serve\"",
    "migrate": "node --loader ts-node/esm src/migrate.ts",
    "script": "source ../solarwind-blockchain/.env && tsc && node dist/runScript.js"
  },
  "author": "",
  "license": "ISC",
  "types": "./types/index.d.ts",
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/cassandra-store": "^5.0.3",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/express-session": "^1.17.7",
    "@types/pg": "^8.10.2",
    "@types/static-kdtree": "^1.0.0",
    "cassandra-driver": "^4.6.4",
    "cassandra-migration": "^3.0.0",
    "cassandra-store": "^5.0.0",
    "connect-pg-simple": "^9.0.0",
    "cors": "^2.8.5",
    "d3-octree": "^1.0.2",
    "ethers": "^6.1.0",
    "express": "^4.18.2",
    "express-openapi": "^12.1.0",
    "express-session": "^1.17.3",
    "ipfs-http-client": "^60.0.0",
    "pg": "^8.11.3",
    "solarwind-blockchain": "0.0.29",
    "solarwind-common": "0.0.18",
    "static-kdtree": "^1.0.2",
    "ts-loader": "^9.4.4"
  }
}
